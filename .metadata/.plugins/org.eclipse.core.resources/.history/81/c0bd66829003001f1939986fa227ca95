package javapratice;
public class Main {
    public static void main(String[] args) {
        // 1~10까지의 합을 구하는 코드
        int sum = 0; // 더해진 수를 저장할 변수
        int totalSum = 0; // 전체 합계를 저장할 변수
        
        // 반복문을 이용하여 1~10까지의 합을 구함
        for (int i = 1; i <= 10; i++) {
            sum += i; // 더해진 수에 현재 수를 더함
            totalSum += i; // 전체 합계에 현재 수를 더함
        }
        
        // 더해진 수와 전체 합계 출력
        System.out.print("더해진 수: " + sum);
        System.out.println(" 전체 합계: " + totalSum);
    }
}
